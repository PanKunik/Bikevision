@model IEnumerable<bikevision.Models.ItemType>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_HeadingLayout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.type)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.type)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idItemType }) |
            @Html.ActionLink("Details", "Details", new { id=item.idItemType }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.idItemType })
        </td>
    </tr>
}

</table>
