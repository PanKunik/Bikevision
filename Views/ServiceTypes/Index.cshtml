@model IEnumerable<bikevision.Models.ServiceType>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_ManagementLayout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.priceFrom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.priceTo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.priceFrom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.priceTo)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idServiceType }) |
            @Html.ActionLink("Details", "Details", new { id=item.idServiceType }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.idServiceType })
        </td>
    </tr>
}

</table>
