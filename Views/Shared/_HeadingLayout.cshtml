<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>@ViewBag.Title - BIKEVISION</title>

    @Styles.Render("~/Content/theme-default.css")
    @Styles.Render("~/Content/shopping-card.css")
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <link href="https://fonts.googleapis.com/css?family=Raleway:400,700&display=swap" rel="stylesheet"> <!-- Raleway Font -->
    <link href="~/Content/Font Awesome 5.10.2/css/all.css" rel="stylesheet" /> <!-- Font Awsome 5.10.2 -->
</head>

<body class="container-fluid d-flex flex-column px-0 py-0 border-0">
    @RenderBody()

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/Scripts/bootstrap-validate.js")
    @RenderSection("scripts", required: false)
</body>
</html>
